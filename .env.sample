# Database Configuration
# For PostgreSQL (recommended for production)
DB_URL=postgresql://username:password@localhost:5432/home_inspection_db

# For SQLite (development)
# DB_URL=sqlite:///./data/home_inspection.db

# OpenAI Configuration
OPENAI_API_KEY=your_openai_api_key_here

# Realtime Model Configuration
REALTIME_MODEL=gpt-4o

# Qdrant Vector Database Configuration
QDRANT_URL=http://localhost:6333
QDRANT_API_KEY=your_qdrant_api_key_here

# Backend API Configuration
BACKEND_HOST=0.0.0.0
BACKEND_PORT=8000

# Frontend Configuration
FRONTEND_PORT=3000

# Development Configuration
NODE_ENV=development
DEBUG=true

# Security
JWT_SECRET_KEY=your_jwt_secret_key_here
CORS_ORIGINS=http://localhost:3000,http://localhost:8000

# Sensor Configuration
SENSOR_DATA_RETENTION_DAYS=30
SENSOR_CALIBRATION_INTERVAL_HOURS=24
